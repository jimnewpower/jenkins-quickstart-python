/* Requires the Docker Pipeline plugin */
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'building jenkins quickstart...'
            }
        }
        stage('test') {
            steps {
                echo 'testing jenkins quickstart...'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying jenkins quickstart...'
            }
        }
    }
}
